{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "remoting": {
    "context": false,
    "sharedMethods": {
        "*": false
    },
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "20mb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "20mb"
    },
    "cors": false,  
    "handleErrors": false
  },
  "path":{
    "imagen_geotecnico":"/var/www/fotos/geotecnico",
    "imagen_ambiental":"/var/www/fotos/ambiental",
    "imagen_incidente":"/var/www/fotos/incidente/",
    "imagen_capacitacion":"/var/www/fotos/capacitacion/"
  }
}
